{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "${workspaceFolder}/tests/fixtures"
      ],
      "outFiles": ["${workspaceFolder}/extension/**/*.js"],
      "preLaunchTask": "npm: vscode:prepublish"
    }
  ]
}
